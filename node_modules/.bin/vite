#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/实训+课程/软件开发实训2/VueProject/vue-project/node_modules/.pnpm/vite@7.1.10_@types+node@24._a671b64f9dada8f8cd2008dd8c97ff51/node_modules/vite/bin/node_modules:/mnt/d/实训+课程/软件开发实训2/VueProject/vue-project/node_modules/.pnpm/vite@7.1.10_@types+node@24._a671b64f9dada8f8cd2008dd8c97ff51/node_modules/vite/node_modules:/mnt/d/实训+课程/软件开发实训2/VueProject/vue-project/node_modules/.pnpm/vite@7.1.10_@types+node@24._a671b64f9dada8f8cd2008dd8c97ff51/node_modules:/mnt/d/实训+课程/软件开发实训2/VueProject/vue-project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/实训+课程/软件开发实训2/VueProject/vue-project/node_modules/.pnpm/vite@7.1.10_@types+node@24._a671b64f9dada8f8cd2008dd8c97ff51/node_modules/vite/bin/node_modules:/mnt/d/实训+课程/软件开发实训2/VueProject/vue-project/node_modules/.pnpm/vite@7.1.10_@types+node@24._a671b64f9dada8f8cd2008dd8c97ff51/node_modules/vite/node_modules:/mnt/d/实训+课程/软件开发实训2/VueProject/vue-project/node_modules/.pnpm/vite@7.1.10_@types+node@24._a671b64f9dada8f8cd2008dd8c97ff51/node_modules:/mnt/d/实训+课程/软件开发实训2/VueProject/vue-project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
